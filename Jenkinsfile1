pipeline
{
	agent
	{
		label 'docker-agent-test-python'
	}
	environment
	{
		appUser = "test"
		appName = "python-docker-test"
		appVersion = "0.0.1"
		buildScript = "python3 hello.py"
		deployScript = 'echo "Deploying..."'
		logScript = 'python3 --version'
	}
	stages
	{
		stage('Build')
		{
			steps {
				sh(script: '''${logScript}''', label: "build")
				sh '''echo "doing build stuff.."'''
			}
		}
	}
}
